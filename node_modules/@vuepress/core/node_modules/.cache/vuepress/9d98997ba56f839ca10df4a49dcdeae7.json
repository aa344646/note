,\n        _c(\"li\", [\n          _vm._v(\n            \"使用HTTPS协议可以进行加密传输、身份认证，通信更加安全，防止数据在传输过程中被窃取、修改，确保数据安全性；\"\n          ),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _vm._v(\n            \"HTTPS是现行架构下最安全的解决方案，虽然不是绝对的安全，但是大幅增加了中间人攻击的成本；\"\n          ),\n        ]),\n      ]),\n      _vm._v(\" \"),\n      _c(\"p\", [\n        _vm._v(\"HTTPS的\"),\n        _c(\"strong\", [_vm._v(\"缺点\")]),\n        _vm._v(\"如下：\"),\n      ]),\n      _vm._v(\" \"),\n      _c(\"ul\", [\n        _c(\"li\", [\n          _vm._v(\n            \"HTTPS需要做服务器和客户端双方的加密个解密处理，耗费更多服务器资源，过程复杂；\"\n          ),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_vm._v(\"HTTPS协议握手阶段比较费时，增加页面的加载时间；\")]),\n        _vm._v(\" \"),\n        _c(\"li\", [_vm._v(\"SSL证书是收费的，功能越强大的证书费用越高；\")]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _vm._v(\n            \"HTTPS连接服务器端资源占用高很多，支持访客稍多的网站需要投入更大的成本；\"\n          ),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_vm._v(\"SSL证书需要绑定IP，不能再同一个IP上绑定多个域名。\")]),\n      ]),\n      _vm._v(\" \"),\n      _c(\"h3\", { attrs: { id: \"_6-https是如何保证安全的\" } }, [\n        _c(\n          \"a\",\n          {\n            staticClass: \"header-anchor\",\n            attrs: { href: \"#_6-https是如何保证安全的\" },\n          },\n          [_vm._v(\"#\")]\n        ),\n        _vm._v(\" 6. \"),\n        _c(\"strong\", [_vm._v(\"HTTPS\")]),\n        _vm._v(\"是如何保证安全的？\"),\n      ]),\n      _vm._v(\" \"),\n      _c(\"p\", [_vm._v(\"先理解两个概念：\")]),\n      _vm._v(\" \"),\n      _c(\"ul\", [\n        _c(\"li\", [\n          _vm._v(\n            \"对称加密：即通信的双⽅都使⽤同⼀个秘钥进⾏加解密，对称加密虽然很简单性能也好，但是⽆法解决⾸次把秘钥发给对⽅的问题，很容易被⿊客拦截秘钥。\"\n          ),\n        ]),\n        _vm._v(\" \"),\n        _c(\"li\", [_vm._v(\"⾮对称加密：\")]),\n      ]),\n      _vm._v(\" \"),\n      _c(\"ol\", [\n        _c(\"li\", [_vm._v(\"私钥 + 公钥= 密钥对\")]),\n        _vm._v(\" \"),\n        _c(\"li\", [\n          _vm._v(\n            \"即⽤私钥加密的数据,只有对应的公钥才能解密,⽤公钥加密的数据,只有对应的私钥才能解密\"\n  